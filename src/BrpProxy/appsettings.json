{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Serilog": "Information"
      }
    }
  },
  "Ecs": {
    "Path": "/var/log/personen-informatie-service.json",
    "FileSizeLimitBytes": "1048576"
  },
  "HealthEndpointBase": "/personen-proxy/health",
  "AllowedHosts": "*",
  "FeatureToggles": {
    "ToelichtingAanleveren":  false
  }
}
